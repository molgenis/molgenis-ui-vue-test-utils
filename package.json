{
  "name": "@molgenis/molgenis-vue-test-utils",
  "version": "1.1.0",
  "description": "Test utilities for Vue and Vuex code",
  "main": "dist/molgenis-vue-test-utils.es.js",
  "module": "dist/molgenis-vue-test-utils.es.js",
  "scripts": {
    "lint": "standard",
    "unit": "babel-node ./node_modules/istanbul/lib/cli.js cover --dir test/unit/coverage ./node_modules/mocha/bin/_mocha test/unit/specs/**/*.js",
    "debug": "mocha --compilers js:babel-register --debug-brk",
    "unit:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "build": "standard; rollup -c",
    "preversion": "yarn install; yarn unit; yarn build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "repository": "git@github.com:molgenis/molgenis-vue-test-utils.git",
  "keywords": [
    "molgenis",
    "vuex",
    "actions",
    "test",
    "vue"
  ],
  "author": "MOLGENIS Team, System Genetics, UMCG, Netherlands",
  "license": "LGPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.0.0-canary.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "standard": "^11.0.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "files": [
    "dist"
  ]
}
